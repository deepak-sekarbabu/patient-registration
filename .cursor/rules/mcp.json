{
    "mcpServers": {
      "mcp-server-mysql": {
        "command": "C:\\Windows\\System32\\cmd.exe",
        "args": [
          "/c",
          "npx",
          "-y",
          "@benborla29/mcp-server-mysql"
        ],
        "env": {
          "MYSQL_HOST": "localhost",
          "MYSQL_PORT": "3306",
          "MYSQL_USER": "root",
          "MYSQL_PASS": "my_password",
          "MYSQL_DB": "QueueManagement",
          "MYSQL_POOL_SIZE": "5",
          "MYSQL_QUERY_TIMEOUT": "30000",
          "MYSQL_CACHE_TTL": "60000",
